{
    "app_name": "Content Management Application",
    "app_version": "1.0.0",
    "app_url": "127.0.0.1",
    "app_base": "/smart-cms",
    "api": {
        "_url": "/api",
        "_version": "/1.0"
    },
    "mongodb": {
        "db": "smart-cms",
        "host": "127.0.0.1",
        "username": "",
        "port": "",
        "collections": {
            "client":"smart-client",
            "page":"smart-page",
            "category":"smart-category",
            "subcategory":"smart-subcategory",
            "tag":"smart-tag",
            "content":"smart-content",
            "asset":"smart-assets"
        }
    },
    "fb": {
        "long_token":"https://graph.facebook.com/oauth/access_token?grant_type=fb_exchange_token&client_id=[APP-ID]&client_secret=[APP-SECRET]&fb_exchange_token=[SHORT-TOKEN]",
        "page_token":"https://graph.facebook.com/[PAGE-ID]?fields=access_token&access_token=[LONG-TOKEN]",
        "publish_post":"https://graph.facebook.com/[PAGE-ID]/feed",
        "delete_post":"https://graph.facebook.com/[PAGE-POST-ID]?access_token=[PAGE-TOKEN]"
    },
    "page_start":1,
    "reset_link":"https://hubitcms.netlify.app/update-password",
    "asset_folder":"./assets",
    "reset_msg":"Password Reset",
    "default_folder":"assets",
    "api_query_limit": 10,
    "rounds":15
}